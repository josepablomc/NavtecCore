@model FE.Identity.Models.Usuarios

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Empleados</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label class="control-label">Nombre Completo</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreCompleto)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Correo</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CorreoUsuario)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Clave Usuario</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ClaveUsuario)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Rol</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Roles.Rol)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdUsuario">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
