@model FE.Identity.Models.Cotizaciones

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Cotizaciones</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label class="control-label">Fecha</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCotizacion)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Nombre</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreCliente)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Precio de Cotizacion</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PrecioCotizacion)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Descripcion de Servicio</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Servicios.DescripcionServicio)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdCotizacion">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
